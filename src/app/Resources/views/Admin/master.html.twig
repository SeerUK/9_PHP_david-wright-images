<!DOCTYPE html>
<html lang="en">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=0.9">
    <title>{% block title %}{% endblock %}{% if block('title') %} - {% endif %} Admin - {{ site_name }}</title>

    {# Start CSS Stylesheets #}

    <link href='{{ asset('http://fonts.googleapis.com/css?family=Open+Sans:300,400,700') }}' rel='stylesheet' type='text/css' />

    {% stylesheets
        'bundles/seerukdwrightcore/css/normalize.css'
        'bundles/seerukdwrightcore/css/flaticons.css'
        'bundles/seerukdwrightcore/css/framework.css'
        'bundles/seerukdwrightcore/css/admin.css'
        filter='cssrewrite' combine=true output='css/adminaggregate.css'
    %}
        <link rel="stylesheet" href="{{ asset_url }}" />
    {% endstylesheets %}

    {% block stylesheets %}{% endblock %}

    {# End CSS Stylesheets #}
</head>
<body>

    {% include ':Admin:header.html.twig' %}

    <div class="container-subnav">
        <div class="inner">
            {% block subnav %}{% endblock %}
        </div>
    </div>

    <div class="container-content">
        {% block body %}{% endblock %}
    </div>

    {% include ':Admin:footer.html.twig' %}

    {# Start JS Scripts #}

    <script src='{{ asset('//ajax.googleapis.com/ajax/libs/jquery/1.9.1/jquery.min.js') }}' type='text/javascript'></script>

    {% javascripts
        '@SeerUKDWrightCoreBundle/Resources/public/js/bootstrap.js'
        combine=true output='css/adminaggregate.js'
    %}
        <script type="text/javascript" src="{{ asset_url }}"></script>
    {% endjavascripts %}

    {% block javascripts %}{% endblock %}

    {# End JS Scripts #}
</body>
</html>
